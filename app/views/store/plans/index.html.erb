<h1>Listing Plans</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
  </tr>
  <% @plans.each do |plan| %>
    <tr>
      <td><%= plan.name %></td>
      <td>
        <%= link_to 'Show', store_plan_path(plan) %> |
        <%= link_to 'Edit', edit_store_plan_path(plan) %> |
        <%= link_to 'Destroy', store_plan_path(plan), confirm: 'Are you sure?', method: :delete %>
      </td>
    </tr>
  <% end %>
</table>
<br />

<%= link_to 'New Plan', new_store_plan_path %>
<br>
<br>
